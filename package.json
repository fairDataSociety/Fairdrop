{
  "name": "fairdrop",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fairdatasociety/fds": "^0.2.0",
    "@walletconnect/client": "^1.7.7",
    "@walletconnect/qrcode-modal": "^1.7.7",
    "classnames": "^2.3.1",
    "filesaver.js": "^1.3.4",
    "formik": "^2.2.9",
    "jszip": "^3.7.1",
    "luxon": "^2.0.2",
    "polished": "^4.1.4",
    "react-dropzone": "^11.4.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.3.0",
    "react-toastify": "^8.0.2",
    "react-tooltip": "^4.2.21",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.5",
    "yup": "^0.32.9"
  },
  "scripts": {
    "pre-commit": "pretty-quick --staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy": "npm run build && docker build -t ghcr.io/mancas/fairdrop . && docker push ghcr.io/mancas/fairdrop",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build:icons": "svg-sprite -s --symbol-dest src/components/atoms/icon/assets --symbol-sprite icons.svg src/assets/icons/*.svg"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/helper-define-map": "^7.16.7",
    "@sentry/browser": "^5.6.1",
    "@sentry/webpack-plugin": "^1.8.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.0",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-dropdown": "^1.5.0",
    "react-scripts": "4.0.3",
    "source-map-explorer": "^2.5.2",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "svg-sprite": "^1.5.4"
  },
  "pre-commit": [
    "pre-commit"
  ],
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
